{
  "version": 2,
  "builds": [
    {
      "src": "./index.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/folders",
      "dest": "/index.js",
      "methods": ["GET"]
    },
    {
      "src": "/api/user-folders",
      "dest": "/index.js",
      "methods": ["GET"]
    }
  ]
}
